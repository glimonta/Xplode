type person{
  int name;
  int id;
  int height;
  int weight;
}

function void print(var person p){
  write("Person Data is : ");
  write(p.id," ", p.name, " ",p.height, " ", p.weight);
}

function void print2(int id){
  write("Person Id is : ");
  write(id, "\n");
}

begin
  let person p;
  read(p.id);
  read(p.height);
  read(p.weight);
  read(p.name);
  print(p);
  print2(p.id);

end
