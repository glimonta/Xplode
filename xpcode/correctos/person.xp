program

type person{
  int id;
  float height;
  float weight;
}


function void print(var person p){
  write("Person Data is : ");
  write(p.id," ",p.height, " ", p.weight);
}

function void print2(int id){
  write("Person Id is : ");
  write(id, "\n");
}

begin
  let person p;
  read(p.id);
  read(p.height);
  read(p.weight);
  print(p);
  print2(p.id);

end
